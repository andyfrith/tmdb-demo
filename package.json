{
  "name": "tmdb-demo",
  "version": "0.3.0",
  "description": "This demo is a minimal Node.js Typescript application.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "tsc": "tsc",
    "postinstall": "npm run tsc",
    "start": "npm run tsc && node --max_old_space_size=128 --inspect ./build/index.js",
    "test": "npm run tsc && jest --clearCache && jest --verbose"
  },
  "author": "Andy Frith",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.2",
    "@types/object-hash": "^1.3.1",
    "@types/pg": "^7.14.3",
    "@types/pino": "^5.17.0",
    "@types/uuid": "^7.0.2",
    "@types/uuid-validate": "0.0.1",
    "@typescript-eslint/eslint-plugin-tslint": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "clear": "^0.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^25.1.0",
    "pino-pretty": "^3.6.1",
    "prettier": "^2.0.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.7.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "date-fns": "^2.11.0",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "express": "^4.17.1",
    "object-hash": "^2.0.3",
    "pg-promise": "^10.4.4",
    "pino": "^5.17.0",
    "pretty-bytes": "^5.3.0",
    "rxjs": "^6.5.4",
    "sinon": "^9.0.1",
    "uglify-js": "^3.8.0",
    "uuid": "^7.0.2",
    "uuid-by-string": "^3.0.2",
    "uuid-validate": "0.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node"
  }
}
